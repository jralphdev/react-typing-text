{
  "name": "@jralphm_dev/react-typing-text",
  "private": false,
  "version": "0.0.1",
  "description": "A lightweight React component for creating smooth typewriter text animations.",
  "type": "module",
  "main": "dist/module/index.umd.js",
  "module": "dist/module/index.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module/index.es.js",
      "types": "./dist/types/index.d.ts",
      "require": "./dist/module/index.umd.js"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev": "cd example && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jralphdev/react-typing-text.git"
  },
  "keywords": [
    "react",
    "typing",
    "typewriter",
    "animation",
    "text",
    "ui"
  ],
  "author": "JRM Dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jralphdev/react-typing-text/issues"
  },
  "homepage": "https://github.com/jralphdev/react-typing-text#readme",
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  }
}
